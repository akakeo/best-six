<Window x:Class="bestsixapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bestsixapp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
         >

    
    <Grid>
        <StackPanel>
            <Button Content="Go to Make a Room" MinWidth="100" Click="RoomClick"/>
            <Button Content="Check in Customer" Click="CustomerClick" />
            <Button Content="Check Out Customer" Click="CheckOutClick"/>

        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden" />


    </Grid>
</Window>
